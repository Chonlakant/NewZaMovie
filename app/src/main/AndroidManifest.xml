<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="newzamovie.com.newzamovie">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- Network State Permissions to detect Internet status -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".ZamovieApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.VideoCacheActivity" />
        <activity android:name=".activity.WebViewActivity" />
        <activity
            android:name=".activity.CartoonActivity"
            android:label="@string/title_activity_cartoon"
            android:theme="@style/AppTheme" />
        <activity android:name=".activity.MusicActivity" />
        <activity
            android:name=".activity.LoadingMovieActivity"
            android:label="@string/title_activity_loading_movie"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.SeActivity"
            android:label="@string/title_activity_se"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>
